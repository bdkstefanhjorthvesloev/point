{
	"name": "department_pipeline",
	"properties": {
		"activities": [
			{
				"name": "department_copy",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "AzureSqlSource",
						"queryTimeout": "02:00:00",
						"partitionOption": "None"
					},
					"sink": {
						"type": "AzureSqlSink",
						"preCopyScript": "TRUNCATE TABLE [dbo].[Department]",
						"disableMetricsCollection": false
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"name": "sys_id_value",
									"type": "String",
									"physicalType": "nvarchar"
								},
								"sink": {
									"name": "Department ID",
									"type": "String",
									"physicalType": "nvarchar"
								}
							},
							{
								"source": {
									"name": "name_value",
									"type": "String",
									"physicalType": "nvarchar"
								},
								"sink": {
									"name": "Department Name",
									"type": "String",
									"physicalType": "nvarchar"
								}
							},
							{
								"source": {
									"name": "company_value",
									"type": "String",
									"physicalType": "nvarchar"
								},
								"sink": {
									"name": "Company ID",
									"type": "String",
									"physicalType": "nvarchar"
								}
							},
							{
								"source": {
									"name": "description_value",
									"type": "String",
									"physicalType": "nvarchar"
								},
								"sink": {
									"name": "Department Description",
									"type": "String",
									"physicalType": "nvarchar"
								}
							},
							{
								"source": {
									"name": "sys_created_on_value",
									"type": "DateTime",
									"physicalType": "datetime2"
								},
								"sink": {
									"name": "Department Created Date",
									"type": "DateTime",
									"physicalType": "datetime2"
								}
							},
							{
								"source": {
									"name": "sys_created_by_value",
									"type": "String",
									"physicalType": "nvarchar"
								},
								"sink": {
									"name": "Department Created by Name",
									"type": "String",
									"physicalType": "nvarchar"
								}
							},
							{
								"source": {
									"name": "sys_updated_on_value",
									"type": "DateTime",
									"physicalType": "datetime2"
								},
								"sink": {
									"name": "Department Updated Date",
									"type": "DateTime",
									"physicalType": "datetime2"
								}
							},
							{
								"source": {
									"name": "sys_updated_by_value",
									"type": "String",
									"physicalType": "nvarchar"
								},
								"sink": {
									"name": "Department Updated by Name",
									"type": "String",
									"physicalType": "nvarchar"
								}
							},
							{
								"source": {
									"name": "id_value",
									"type": "String",
									"physicalType": "nvarchar"
								},
								"sink": {
									"name": "ID",
									"type": "String",
									"physicalType": "nvarchar"
								}
							}
						],
						"typeConversion": true,
						"typeConversionSettings": {
							"allowDataTruncation": true,
							"treatBooleanAsNumber": false
						}
					}
				},
				"inputs": [
					{
						"referenceName": "stage_cmn_department_table",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "department_table",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}